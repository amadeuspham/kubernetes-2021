apiVersion: extensions/v1beta1
kind: Ingress
metadata:
  name: {{ ingress_name }}
  labels:
    dummysite: "{{ dummysite_name }}"
  annotations:
    traefik.ingress.kubernetes.io/rule-type: "PathPrefixStrip"
spec:
  rules:
  - http:
      paths:
      - path: /
        backend:
          serviceName: {{ service_name }}
          servicePort: {{ service_port }}